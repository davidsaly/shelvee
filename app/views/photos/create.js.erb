<% if @photo.new_record? %>
  alert("Failed to upload the photo: <%= j @photo.errors.full_messages.join(', ').html_safe %>");
<% else %>
	var $newPhoto = $("<%= j render (@photo) %>");
  	$("#to-be-appended").append($newPhoto); //.masonry( 'appended', $newPhoto, true).masonry( 'reload' );
  	$('.image_picker_selector').masonry( 'reload' )
  	//$('.grid').masonry( 'reloadItems' )
<% end %>
